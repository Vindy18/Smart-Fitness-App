<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/trainer"
    tools:context=".Admin_View_WeightTrainer_Home">

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="531dp"
        android:layout_height="190dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/box_container"
        tools:layout_editor_absoluteY="253dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="269dp"
        android:layout_height="86dp"
        android:text="Total No.of Trainers"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="299dp" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="178dp"
        android:layout_height="153dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <com.google.android.material.tabs.TabLayout
        android:layout_width="609dp"
        android:layout_height="62dp"
        android:background="@color/Tab_color"
        app:tabIndicatorColor="@color/black"
        app:tabSelectedTextColor="#100F0F"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="174dp">

        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cardio Instructor" />

        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Weight Trainer" />

        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nutrition Instructor" />
    </com.google.android.material.tabs.TabLayout>

    <Button
        android:id="@+id/button11"
        android:layout_width="347dp"
        android:layout_height="85dp"
        android:text="Add Trainer"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:backgroundTint="@color/white"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="439dp" />

    <Button
        android:id="@+id/button14"
        android:layout_width="347dp"
        android:layout_height="85dp"
        android:text="Modify Trainer Details"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:backgroundTint="@color/white"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="538dp" />

    <Button
        android:id="@+id/button12"
        android:layout_width="347dp"
        android:layout_height="85dp"
        android:text="Delete Trainer"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:backgroundTint="@color/white"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="634dp" />

    <Button
        android:id="@+id/button13"
        android:layout_width="347dp"
        android:layout_height="85dp"
        android:text="View Trainers"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:backgroundTint="@color/white"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="745dp" />

    <EditText
        android:id="@+id/editTextTextPersonName23"
        android:layout_width="103dp"
        android:layout_height="75dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="750"
        android:textColor="@color/black"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="381dp"
        tools:layout_editor_absoluteY="310dp" />

</androidx.constraintlayout.widget.ConstraintLayout>